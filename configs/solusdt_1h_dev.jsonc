{
  "train": true,
  "venue": "binance",
  "api_key": "",
  "api_secret": "",
  "telegram_bot_token": "",
  "telegram_chat_id": "",
  "data_folder": "./DATA_ITB_1h",
  "symbol": "SOLUSDT",
  "description": "SOLUSDT 1 hour strategy",
  "freq": "1h",
  "pandas_freq": "1h",
  "feature_file_name": "features.csv",
  "merge_file_name":   "data.csv",
  "matrix_file_name":  "matrix.csv",
  "label_horizon":   24,
  "features_horizon":24,
  "train_length":    8760,
  "predict_length":  168,
  "append_overlap_records": 5,
  "data_sources": [
    { "folder": "SOLUSDT", "file": "klines", "column_prefix": "" }
  ],
  "download_start": "2022-01-01",
  "download_end":   null,
  "merge_interpolate": false,
  "feature_sets": [
    { "generator": "talib", "config": { "columns": ["close"], "functions": ["SMA"], "windows": [24, 72, 168, 720] } },
    { "generator": "talib", "config": { "columns": ["close"], "functions": ["RSI"], "windows": [14] } },
    { "generator": "talib", "config": { "columns": ["close"], "functions": ["LINEARREG_SLOPE"], "windows": [24, 72, 168] } },
    { "generator": "talib", "config": { "columns": ["high","low","close"], "functions": ["ATR"], "windows": [14] } },
    { "generator": "talib", "config": { "columns": ["close"], "functions": ["STDDEV"], "windows": [24, 168] } }
  ],
  "label_sets": [
    { "generator": "highlow2", "config": { "columns": ["close","high","low"], "function": "high", "thresholds": [2.0], "tolerance": 0.2, "horizon": 12, "names": ["high_30"] } },
    { "generator": "highlow2", "config": { "columns": ["close","high","low"], "function": "low", "thresholds": [2.0], "tolerance": 0.2, "horizon": 12, "names": ["low_30"] } }
  ],
  "train_feature_sets": [ { "generator": "train_features", "config": { } } ],
  "train_features": [
    "close_SMA_24","close_SMA_72","close_SMA_168","close_SMA_720",
    "close_RSI_14",
    "close_LINEARREG_SLOPE_24","close_LINEARREG_SLOPE_72","close_LINEARREG_SLOPE_168",
    "high_low_close_ATR_14","close_STDDEV_24","close_STDDEV_168"
  ],
  "labels": ["high_30","low_30"],
  "algorithms": [
    { "name": "lc", "algo": "lc", "params": { "is_scale": true }, "train": { "penalty": "l2", "C": 1.0, "solver": "sag", "max_iter": 500 } }
  ],
  "signal_sets": [
    { "generator": "combine", "config": { "columns": ["high_30_lc","low_30_lc"], "names": "trade_score", "combine": "difference" } },
    { "generator": "threshold_rule", "config": { "columns": "trade_score", "names": ["buy_signal","sell_signal"], "parameters": { "buy_signal_threshold": 0.02, "sell_signal_threshold": -0.02 } } }
  ],
  "output_sets": [
    { "generator": "score_notification_model", "config": { "score_notification": true, "score_column_names": ["trade_score"] } },
    { "generator": "trader_simulation", "config": { "buy_signal_column": "buy_signal", "sell_signal_column": "sell_signal" } }
  ],
  "simulate_model": {
    "data_start": 0, "data_end": null, "direction": "long", "topn_to_store": 10,
    "signal_generator": "threshold_rule", "buy_sell_equal": false,
    "grid": { "buy_signal_threshold":  [0.015, 0.025, 0.035], "sell_signal_threshold": [-0.015, -0.025, -0.035] }
  }
}
