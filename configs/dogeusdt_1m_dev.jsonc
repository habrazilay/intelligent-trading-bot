{
  "train": true,
  "venue": "binance",
  "api_key": "",
  "api_secret": "",
  "telegram_bot_token": "",
  "telegram_chat_id": "",
  "data_folder": "./DATA_ITB_1m",
  "symbol": "DOGEUSDT",
  "description": "DOGEUSDT 1 minute strategy",
  "freq": "1m",
  "pandas_freq": "1min",
  "feature_file_name": "features.csv",
  "merge_file_name":   "data.csv",
  "matrix_file_name":  "matrix.csv",
  "label_horizon":   15,
  "features_horizon":1440,
  "train_length":    300000,
  "predict_length":  10080,
  "append_overlap_records": 5,
  "data_sources": [
    { "folder": "DOGEUSDT", "file": "klines", "column_prefix": "" }
  ],
  "download_start": "2022-01-01",
  "download_end":   null,
  "merge_interpolate": false,
  "feature_sets": [
    { "generator": "talib", "config": { "columns": ["close"], "functions": ["SMA"], "windows": [60, 240, 1440, 10080] } },
    { "generator": "talib", "config": { "columns": ["close"], "functions": ["RSI"], "windows": [14] } },
    { "generator": "talib", "config": { "columns": ["close"], "functions": ["LINEARREG_SLOPE"], "windows": [60, 240, 1440] } },
    { "generator": "talib", "config": { "columns": ["high","low","close"], "functions": ["ATR"], "windows": [14] } },
    { "generator": "talib", "config": { "columns": ["close"], "functions": ["STDDEV"], "windows": [60, 1440] } }
  ],
  "label_sets": [
    { "generator": "highlow2", "config": { "columns": ["close","high","low"], "function": "high", "thresholds": [1.0], "tolerance": 0.1, "horizon": 15, "names": ["high_10"] } },
    { "generator": "highlow2", "config": { "columns": ["close","high","low"], "function": "low", "thresholds": [1.0], "tolerance": 0.1, "horizon": 15, "names": ["low_10"] } }
  ],
  "train_feature_sets": [ { "generator": "train_features", "config": { } } ],
  "train_features": [
    "close_SMA_60","close_SMA_240","close_SMA_1440","close_SMA_10080",
    "close_RSI_14",
    "close_LINEARREG_SLOPE_60","close_LINEARREG_SLOPE_240","close_LINEARREG_SLOPE_1440",
    "high_low_close_ATR_14","close_STDDEV_60","close_STDDEV_1440"
  ],
  "labels": ["high_10","low_10"],
  "algorithms": [
    { "name": "lc", "algo": "lc", "params": { "is_scale": true }, "train": { "penalty": "l2", "C": 1.0, "solver": "sag", "max_iter": 500 } }
  ],
  "signal_sets": [
    { "generator": "combine", "config": { "columns": ["high_10_lc","low_10_lc"], "names": "trade_score", "combine": "difference" } },
    { "generator": "threshold_rule", "config": { "columns": "trade_score", "names": ["buy_signal","sell_signal"], "parameters": { "buy_signal_threshold": 0.01, "sell_signal_threshold": -0.01 } } }
  ],
  "output_sets": [
    { "generator": "score_notification_model", "config": { "score_notification": true, "score_column_names": ["trade_score"] } },
    { "generator": "trader_simulation", "config": { "buy_signal_column": "buy_signal", "sell_signal_column": "sell_signal" } }
  ],
  "simulate_model": {
    "data_start": 0, "data_end": null, "direction": "long", "topn_to_store": 10,
    "signal_generator": "threshold_rule", "buy_sell_equal": false,
    "grid": { "buy_signal_threshold":  [0.008, 0.01, 0.012], "sell_signal_threshold": [-0.008, -0.01, -0.012] }
  }
}
